#Entrar com três números e imprimi-los em ordem crescente (suponha números diferentes).
prog algoritmo117
     int a, b, c, aux;
     imprima "Digite um numero: ";
     leia a;
     imprima "Digite outro numero: ";
     leia b;
     imprima "Digite outro numero: ";
     leia c;
     
     se (a > b)
     {
         aux <- a;
         a <-b;
         b <- aux;   
     }
     
     se (a > c)
     {
          aux <- a;
          a <- c;
          c <- aux;
       
     }
     
     se (b > c)
     {
      aux <- b;
      b <- c;
      c <- aux;     
     }
     
     
     imprima "Ordem: ", a, " ", b, " ", c, " \n";
           #A logica e ir realocando os maiores numeros e deixando na ordem que devem ser apresentados, verificando as possibilidades.

fimprog