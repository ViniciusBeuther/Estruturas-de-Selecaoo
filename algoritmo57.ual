#Entrar com as notas da PR1 e PR2 e imprimir a média final: trundaca e arredondada

prog algoritmo57
       real n1,n2,mf;
       imprima "\nEntre com a nota da prova 01: ";
       leia n1;
       imprima "\nEntre com a nota da prova 02: ";
       leia n2;
       mf <- (n1+n2)/2;
       imprima "\nMedia truncada: ", realint((mf - 0.5) + 0.001);
       imprima "\nMedia arredondada: ", realint (mf+0.001);
       imprima "\n";
fimprog